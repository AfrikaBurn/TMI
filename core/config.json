{
	"name": "Tribe Core",
	"port": 3000,

	"services": {

		"user":{
			"schema": "user.schema",
			"salt": "default salt",
			"controller": "TmiUserController",
			"stash": "TmiUserStash"
		},

		"agreement":{
			"schema": "agreement.schema",
			"controller": "TmiAgreementController",
			"stash": "TmiAgreementStash"
		},

		"collective":{
			"schema": "collective.schema",
			"controller": "TmiCollectiveController",
			"stash": "TmiCollectiveStash"
		},

		"post":{
			"schema": "post.schema",
			"controller": "TmiPostController",
			"stash": "TmiPostStash"
		},

		"profile":{
			"schema": "profile.schema",
			"controller": "TmiProfileController",
			"stash": "TmiProfileStash"
		}

	},

	"origins": ["http://localhost:4200"]
}