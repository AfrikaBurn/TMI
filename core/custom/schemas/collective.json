{
  "type": "object",
  "title": "Collective",
  "properties": {

    "name": {
      "title": "Name",
      "type": "string"
    },

    "description": {
      "title": "Description",
      "type": "string"
    },

    "status": {
      "title": "string",
      "enum": ["draft", "active", "retired"]
    },

    "revision": {
      "type": "number"
    },

    "created": "",

    "profiles": {
      "title": "Collective profiles",
      "type": "array",
      "description": "Profiles of this collective.",
      "items": {
        "type": "number"
      }
    },

    "agreements": {
      "title": "Agreements",
      "type": "array",
      "description": "Agreements available between members of this collective.",
      "items": {
        "type": "object",
        "properties": {
          "agreementId":  {
            "type": "number"
          },
          "required":  {
            "type": "boolean"
          },
          "cardinality":  {
            "type": "number"
          }
        }
      }
    },

    "visibility": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["public", "tribe", "private"]
      }
    },

    "membership": {
      "type": {
        "type": "string",
        "enum": ["open", "closed"]
      }
    }
  }
}